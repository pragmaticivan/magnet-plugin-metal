{
  "name": "magnet-plugin-metal",
  "version": "1.2.0-alpha.1",
  "description": "Magnet plugin for metaljs support",
  "engines": {
    "node": ">=7.6.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src",
    "render.js"
  ],
  "scripts": {
    "build": "babel --presets es2015 -d lib/ src/",
    "format": "prettier --single-quote --no-bracket-spacing --trailing-comma es5 --write \"{src/**/*.js}\" && eslint --fix \"{src/**/*.js}\"",
    "lint": "eslint src/**/*.js",
    "prepublish": "npm run build",
    "test": "mocha --require babel-core/register test/setup/node.js test/unit/**/*.js"
  },
  "contributors": [
    "Eduardo Lundgren <eduardo.lundgren@liferay.com>",
    "Ivan Santos <ivan.santos@liferay.com>"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/wedeploy/magnet-plugin-metal.git"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-metal-jsx": "0.1.1",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "metal": "^2.14.0",
    "metal-assertions": "^2.14.0",
    "metal-component": "^2.14.1",
    "metal-dom": "^2.14.0",
    "metal-jsx": "^2.14.1",
    "metal-router": "^3.6.3",
    "metal-soy": "^2.14.1",
    "metal-tools-soy": "^4.0.1",
    "node-esapi": "0.0.1",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-register": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-liferay": "^2.0.10",
    "magnet": "^2.0.0",
    "mocha": "^4.0.1",
    "prettier": "^1.8.2"
  }
}
